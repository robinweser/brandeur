{
  "name": "brandeur-plugin-debug",
  "version": "1.0.2",
  "description": "Brandeur plugin to debug layouts",
  "repository": "https://github.com/robinweser/brandeur.git",
  "author": "Robin Weser",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "browserslist": "> 0.25%, not dead",
  "files": [
    "LICENSE",
    "README.md",
    "index.d.ts",
    "dist/**"
  ],
  "keywords": [
    "brandeur",
    "dynamic styling",
    "inline style",
    "css-hooks",
    "brandeur plugin",
    "debugger"
  ],
  "scripts": {
    "setup": "pnpm build",
    "clean": "rimraf dist coverage",
    "build": "pnpm clean && babel src --out-dir dist --ignore **/__tests__",
    "dev": "babel src -d dist -w",
    "test": "ava"
  },
  "dependencies": {
    "styles-debugger": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.8",
    "ava": "^6.1.3",
    "rimraf": "^5.0.5"
  }
}
