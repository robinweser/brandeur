{
  "name": "brandeur",
  "version": "1.0.2",
  "description": "Styling tool belt on top of css-hooks and fela plugins",
  "repository": "https://github.com/robinweser/brandeur.git",
  "author": "Robin Weser",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "browserslist": "> 0.25%, not dead",
  "files": [
    "LICENSE",
    "README.md",
    "index.d.ts",
    "dist/**"
  ],
  "keywords": [
    "brandeur",
    "dynamic styling",
    "inline style",
    "css-hooks",
    "css",
    "styling",
    "functional styling",
    "functional css",
    "state-driven",
    "state-driven styling",
    "cssinjs"
  ],
  "scripts": {
    "setup": "pnpm build",
    "clean": "rimraf dist coverage",
    "build": "pnpm clean && babel src --out-dir dist --ignore **/__tests__",
    "dev": "babel src -d dist -w",
    "test": "echo 0"
  },
  "dependencies": {
    "@types/react": "^18.3.3",
    "css-in-js-utils": "^3.1.0",
    "fast-loops": "^1.1.3",
    "isobject": "^4.0.0"
  },
  "peerDependencies": {
    "@css-hooks/core": "^1.0.0",
    "@css-hooks/react": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.8",
    "@css-hooks/core": "^1.8.2",
    "@css-hooks/react": "^1.8.2",
    "ava": "^6.1.3",
    "cssbeautify": "^0.3.1",
    "rimraf": "^5.0.5"
  }
}
